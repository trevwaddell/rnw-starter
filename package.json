    "html-webpack-plugin": "^3.2.0",
    "jest": "^23.4.1",
    "rimraf": "^2.6.2",
    "ts-loader": "^5.3.2",
    "typescript": "^3.2.2",
    "webpack": "^4.16.0",
    "webpack-bundle-analyzer": "^2.13.1",
    "webpack-cli": "^3.0.8",
    "webpack-dev-server": "^3.1.4",
    "webpack-merge": "^4.1.3"
