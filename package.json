{
  "name": "rnw-starter",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "webpack": "webpack",
    "webpack-dev-server": "webpack-dev-server",
    "clean": "rimraf dist",
    "dev": "yarn clean && yarn webpack-dev-server --env.mode development --hot",
    "dev:compress": "yarn dev --env.presets compress",
    "prod": "yarn clean && yarn webpack -p --env.mode production",
    "prod:analyze": "yarn prod --env.presets analyze",
    "prod:compress": "yarn prod --env.presets compress",
    "test": "jest",
    "test:cover": "yarn test --coverage",
    "test:dev": "yarn test --watch --coverage"
  },
  "dependencies": {
    "react": "^16.4.1",
    "react-dom": "^16.4.1",
    "react-native-web": "^0.8.8",
    "react-router-dom": "^4.3.1"
  },
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.5",
    "babel-plugin-module-resolver": "^3.1.1",
    "babel-plugin-react-native-web": "^0.8.8",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "compression-webpack-plugin": "^1.1.11",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^23.4.1",
    "rimraf": "^2.6.2",
    "webpack": "^4.16.0",
    "webpack-bundle-analyzer": "^2.13.1",
    "webpack-cli": "^3.0.8",
    "webpack-dev-server": "^3.1.4",
    "webpack-merge": "^4.1.3"
  }
}
