  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "webpack": "webpack",
    "webpack-dev-server": "webpack-dev-server",
    "clean": "rimraf dist",
    "dev": "yarn clean && yarn webpack-dev-server --env.mode development --hot",
    "dev:ts": "yarn dev --env.presets typescript",
    "prod": "yarn clean && yarn webpack --env.mode production",
    "prod:analyze": "yarn prod --env.presets analyze",
    "prod:compress": "yarn prod --env.presets compress",
    "test": "jest",
    "test:dev": "yarn test --watch --coverage"
  },
  "dependencies": {
    "@types/react": "^16.7.18",
    "@types/react-dom": "^16.0.11",
    "react": "16.7.0",
    "react-art": "16.5.1",
    "react-dom": "16.7.0",
    "react-native-web": "0.11.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.1.2",
    "@babel/core": "^7.0.0",
    "@babel/plugin-proposal-class-properties": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^8.2.6",
    "babel-jest": "^23.4.0",
    "babel-loader": "^8.0.0",
    "babel-plugin-module-resolver": "^3.1.1",
    "babel-plugin-react-native-web": "^0.8.8",
    "compression-webpack-plugin": "^1.1.11",
